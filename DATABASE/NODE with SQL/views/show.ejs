<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>All Users</title>
    <style>
        table,th,td,tr{
            border: 1px solid black;
        }
    </style>
</head>
<body>
    <table>
        <tr>
            <th>id</th>
            <th>username</th>
            <th>email</th>
            <th>password</th>
        </tr>

<%for(user of users){%>
    <tr>
        <td><%=user.id%></td>
        <td><%=user.username%></td>
        <td><%=user.email%></td>
         <td><%=user.password%></td>
         <td> <form action="/users/<%=user.id%>/edit" method="get">
        <button>Edit</button>
    </form></td>
    <td><form  method="post" action="/users/<%=user.id%>?_method=DELETE">
        <button>Delete</button>
    </form></td>
    </tr>
   
<%}%>
    </table>
</body>
</html>

